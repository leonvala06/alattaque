const express = require('express');
const router = express.Router();
const stuffCtrl = require('../controllers/stuff');

router.post('/', stuffCtrl.createPlateau);
router.get('/:id', stuffCtrl.getOneCarre);
router.put('/:id', stuffCtrl.modifyCarre);
router.delete('/:id', stuffCtrl.deletePlateau);

module.exports = router;